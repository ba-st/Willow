tests-rendering
testofTypeBase64EncodedAsApplying

	| image html |

	image := ImageWebView
		ofType: WAMimeType imagePng
		base64EncodedAs:
			''
		applying: CompositeComponentCommand empty.

	html := self render: image.

	self
		assert: html
		equals:
			'<img alt="" src="data:image/png;base64,"/>'